<Application
    x:Class="JeffTools.CDRTool.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:JeffTools.CDRTool"
    StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="IconFont" Source="Styles/IconFontResource.xaml" />
                <ResourceDictionary x:Name="TabControlStyle" Source="Styles/TabControl.xaml" />
                <ResourceDictionary x:Name="CloseButtonStyle" Source="Styles/Button.xaml" />
                <ResourceDictionary x:Name="ToggleButtonStyle" Source="Styles/ToggleButton.xaml" />
                <ResourceDictionary x:Name="ColorRadioButtonStyle" Source="/Styles/ColorRadioButton.xaml" />
                <ResourceDictionary x:Name="ExpanderStyle" Source="/Styles/Expander.xaml" />
                <ResourceDictionary x:Name="ComboBoxStyle" Source="/Styles/ComboBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
